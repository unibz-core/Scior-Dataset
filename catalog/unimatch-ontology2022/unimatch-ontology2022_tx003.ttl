@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/College> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Educational_Organization> .

<http://taxonomy.model/Institute> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Educational_Organization> .

<http://taxonomy.model/University> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Educational_Organization> .

<http://taxonomy.model/Vocational_school> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Educational_Organization> .

<http://taxonomy.model/Educational_Organization> a owl:Class .

