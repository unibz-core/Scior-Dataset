@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/bachelorStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/masterStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/phdStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/postAuthor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/user> .

<http://taxonomy.model/postInteractor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/user> .

<http://taxonomy.model/professor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/profileInteractor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/user> .

<http://taxonomy.model/sportOrganizationMember> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/teachingAssistant> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/user> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/person> .

<http://taxonomy.model/person> a owl:Class .

