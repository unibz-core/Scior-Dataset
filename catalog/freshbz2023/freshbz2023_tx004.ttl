@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:bachelorStudent a owl:Class ;
    rdfs:subClassOf :person .

:masterStudent a owl:Class ;
    rdfs:subClassOf :person .

:phdStudent a owl:Class ;
    rdfs:subClassOf :person .

:postAuthor a owl:Class ;
    rdfs:subClassOf :user .

:postInteractor a owl:Class ;
    rdfs:subClassOf :user .

:professor a owl:Class ;
    rdfs:subClassOf :person .

:profileInteractor a owl:Class ;
    rdfs:subClassOf :user .

:sportOrganizationMember a owl:Class ;
    rdfs:subClassOf :person .

:teachingAssistant a owl:Class ;
    rdfs:subClassOf :person .

:user a owl:Class ;
    rdfs:subClassOf :person .

:person a owl:Class .

