@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/postComment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/postInteraction> .

<http://taxonomy.model/postRating> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/postInteraction> .

<http://taxonomy.model/postSearch> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/postInteraction> .

<http://taxonomy.model/postInteraction> a owl:Class .

