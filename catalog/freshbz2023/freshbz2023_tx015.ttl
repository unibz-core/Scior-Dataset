@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/sportOrganization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/organization> .

<http://taxonomy.model/store> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/organization> .

<http://taxonomy.model/university> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/EducationalOrganization> .

<http://taxonomy.model/EducationalOrganization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/organization> .

<http://taxonomy.model/organization> a owl:Class .

