@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/clothing> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/product> .

<http://taxonomy.model/food> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/product> .

<http://taxonomy.model/productDiscount> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/product> .

<http://taxonomy.model/productRegularPrice> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/product> .

<http://taxonomy.model/services> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/product> .

<http://taxonomy.model/product> a owl:Class .

