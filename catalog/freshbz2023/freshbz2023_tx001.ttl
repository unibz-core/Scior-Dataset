@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:bathroomAmenity a owl:Class ;
    rdfs:subClassOf :amenity .

:bedroomAmentity a owl:Class ;
    rdfs:subClassOf :amenity .

:entertainmentAmentity a owl:Class ;
    rdfs:subClassOf :amenity .

:kitchenAmenity a owl:Class ;
    rdfs:subClassOf :amenity .

:amenity a owl:Class .

