@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Base_Table a owl:Class ;
    rdfs:subClassOf :Table .

:Check_Constraint a owl:Class ;
    rdfs:subClassOf :Column_Constraint .

:Column_Type_Constraint a owl:Class ;
    rdfs:subClassOf :Column_Constraint .

:DBMS_Account a owl:Class ;
    rdfs:subClassOf :DBMS_Item .

:Data_File a owl:Class ;
    rdfs:subClassOf :Software_Item .

:Data_Schema a owl:Class ;
    rdfs:subClassOf :Schema .

:Data_Type_Constraint a owl:Class ;
    rdfs:subClassOf :Constraint .

:Derived_Table a owl:Class ;
    rdfs:subClassOf :Table .

:Dictionary a owl:Class ;
    rdfs:subClassOf :DBMS_Item .

:Foreign_Key_Constraint a owl:Class ;
    rdfs:subClassOf :Column_Constraint .

:Information_Schema a owl:Class ;
    rdfs:subClassOf :Schema .

:Primary_Key_Constraint a owl:Class ;
    rdfs:subClassOf :Column_Constraint .

:RDBMS a owl:Class ;
    rdfs:subClassOf :DBMS .

:Relational_Database a owl:Class ;
    rdfs:subClassOf :Database,
        :RDBMS_Item .

:Relational_Schema a owl:Class ;
    rdfs:subClassOf :RDBMS_Item,
        :Schema .

:Artifact a owl:Class .

:DBMS a owl:Class ;
    rdfs:subClassOf :Software_System .

:Database a owl:Class ;
    rdfs:subClassOf :DBMS_Item .

:Software_System a owl:Class ;
    rdfs:subClassOf :Software_Item .

:Constraint a owl:Class ;
    rdfs:subClassOf :RDBMS_Item .

:Table a owl:Class ;
    rdfs:subClassOf :RDBMS_Item .

:Schema a owl:Class ;
    rdfs:subClassOf :DBMS_Item .

:Software_Item a owl:Class ;
    rdfs:subClassOf :Artifact .

:Column_Constraint a owl:Class ;
    rdfs:subClassOf :Constraint .

:RDBMS_Item a owl:Class ;
    rdfs:subClassOf :DBMS_Item .

:DBMS_Item a owl:Class ;
    rdfs:subClassOf :Software_Item .

