@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Cardinal_Nominator a owl:Class ;
    rdfs:subClassOf :Pope .

:Deacon a owl:Class ;
    rdfs:subClassOf :Secular_Clergy_Member .

:Layman a owl:Class ;
    rdfs:subClassOf :Regular_Clergy_Member .

:Ordained_Bishop a owl:Class ;
    rdfs:subClassOf :Bishop .

:Ordinator_of_Bishop a owl:Class ;
    rdfs:subClassOf :Bishop .

:Ordinator_of_Deacon a owl:Class ;
    rdfs:subClassOf :Bishop .

:Ordinator_of_Priest a owl:Class ;
    rdfs:subClassOf :Bishop .

:Priest a owl:Class ;
    rdfs:subClassOf :Secular_Clergy_Member .

:Witness_of_Profession a owl:Class ;
    rdfs:subClassOf :Clergyman .

:Cardinal a owl:Class ;
    rdfs:subClassOf :Secular_Clergy_Member .

:Clergyman a owl:Class ;
    rdfs:subClassOf :Regular_Clergy_Member .

:Person a owl:Class .

:Pope a owl:Class ;
    rdfs:subClassOf :Cardinal .

:Clergy_Member a owl:Class ;
    rdfs:subClassOf :Person .

:Regular_Clergy_Member a owl:Class ;
    rdfs:subClassOf :Clergy_Member .

:Bishop a owl:Class ;
    rdfs:subClassOf :Secular_Clergy_Member .

:Secular_Clergy_Member a owl:Class ;
    rdfs:subClassOf :Clergy_Member .

