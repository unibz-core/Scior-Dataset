@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Coding_Region> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Nucleotide_Subsequence> .

<http://taxonomy.model/Codon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Nucleotide_Subsequence> .

<http://taxonomy.model/Full_Consensus_Sequence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Virus_Sequence> .

<http://taxonomy.model/Protein> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sequence> .

<http://taxonomy.model/Reference_Sequence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sequence> .

<http://taxonomy.model/Regular_Sequence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sequence> .

<http://taxonomy.model/Nucleotide_Subsequence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Virus_Sequence> .

<http://taxonomy.model/Virus_Sequence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sequence> .

<http://taxonomy.model/Sequence> a owl:Class .

