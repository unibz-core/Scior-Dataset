@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Coding_Region a owl:Class ;
    rdfs:subClassOf :Nucleotide_Subsequence .

:Codon a owl:Class ;
    rdfs:subClassOf :Nucleotide_Subsequence .

:Full_Consensus_Sequence a owl:Class ;
    rdfs:subClassOf :Virus_Sequence .

:Protein a owl:Class ;
    rdfs:subClassOf :Sequence .

:Reference_Sequence a owl:Class ;
    rdfs:subClassOf :Sequence .

:Regular_Sequence a owl:Class ;
    rdfs:subClassOf :Sequence .

:Nucleotide_Subsequence a owl:Class ;
    rdfs:subClassOf :Virus_Sequence .

:Virus_Sequence a owl:Class ;
    rdfs:subClassOf :Sequence .

:Sequence a owl:Class .

