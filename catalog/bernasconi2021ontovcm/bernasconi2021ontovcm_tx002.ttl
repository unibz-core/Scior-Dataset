@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Analyst a owl:Class ;
    rdfs:subClassOf :Person .

:Author a owl:Class ;
    rdfs:subClassOf :Researcher .

:Bat a owl:Class ;
    rdfs:subClassOf :Animal .

:In_Vitro_Host a owl:Class ;
    rdfs:subClassOf :Cell_Line,
        :Virus_Host .

:Living_Host a owl:Class ;
    rdfs:subClassOf :Animal,
        :Virus_Host .

:Project_Participant a owl:Class ;
    rdfs:subClassOf :Researcher .

:Sampled_Animal a owl:Class ;
    rdfs:subClassOf :Animal .

:Virus a owl:Class ;
    rdfs:subClassOf :Biological_Organism .

:Cell_Line a owl:Class .

:Biological_Organism a owl:Class .

:Person a owl:Class ;
    rdfs:subClassOf :Animal .

:Researcher a owl:Class ;
    rdfs:subClassOf :Person .

:Virus_Host a owl:Class .

:Animal a owl:Class ;
    rdfs:subClassOf :Biological_Organism .

