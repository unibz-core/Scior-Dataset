@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Blocked_Load> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Obstacled_Sediment> .

<http://taxonomy.model/Captured_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Obstacled_Sediment> .

<http://taxonomy.model/Frictioned_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Unstable_Sediment> .

<http://taxonomy.model/Transported_Load> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Drawn_Sediment> .

<http://taxonomy.model/Deposited_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sediment> .

<http://taxonomy.model/Unstable_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Deposited_Sediment> .

<http://taxonomy.model/Drawn_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sediment> .

<http://taxonomy.model/Obstacled_Sediment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Drawn_Sediment> .

<http://taxonomy.model/Sediment> a owl:Class .

