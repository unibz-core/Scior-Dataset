@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/from> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/outside-party> .

<http://taxonomy.model/inside-party> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/participation> .

<http://taxonomy.model/to> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/outside-party> .

<http://taxonomy.model/outside-party> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/participation> .

<http://taxonomy.model/participation> a owl:Class .

