@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:consume a owl:Class ;
    rdfs:subClassOf :decrement-event,
        :transformation .

:give a owl:Class ;
    rdfs:subClassOf :decrement-event,
        :transfer .

:produce a owl:Class ;
    rdfs:subClassOf :increment-event,
        :transformation .

:take a owl:Class ;
    rdfs:subClassOf :increment-event,
        :transfer .

:decrement-event a owl:Class ;
    rdfs:subClassOf :economic-event .

:economic-event a owl:Class .

:increment-event a owl:Class ;
    rdfs:subClassOf :economic-event .

:transfer a owl:Class .

:transformation a owl:Class .

