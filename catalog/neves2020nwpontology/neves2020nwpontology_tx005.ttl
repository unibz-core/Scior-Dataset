@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Geographic_Coverage> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Spatial_Thing> .

<http://taxonomy.model/Line> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Geographic_Object> .

<http://taxonomy.model/Point> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Geographic_Object> .

<http://taxonomy.model/Polygon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Geographic_Object> .

<http://taxonomy.model/Spatial_Thing> a owl:Class .

<http://taxonomy.model/Geographic_Object> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Spatial_Thing> .

