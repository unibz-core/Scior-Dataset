@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Reservoir a owl:Class ;
    rdfs:subClassOf :LithologicUnit_qua_sandstone .

:Seal a owl:Class ;
    rdfs:subClassOf :LithologicUnit_qua_shale .

:Source_Rock a owl:Class ;
    rdfs:subClassOf :LithologicUnit_qua_shale .

:LithologicUnit_qua_sandstone a owl:Class ;
    rdfs:subClassOf :Lithological_unit .

:LithologicUnit_qua_shale a owl:Class ;
    rdfs:subClassOf :Lithological_unit .

:Lithological_unit a owl:Class .

