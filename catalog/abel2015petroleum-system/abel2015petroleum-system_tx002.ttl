@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Sandstone> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Siliciclastic_Rock> .

<http://taxonomy.model/Shale> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Siliciclastic_Rock> .

<http://taxonomy.model/Rock> a owl:Class .

<http://taxonomy.model/Siliciclastic_Rock> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Rock> .

