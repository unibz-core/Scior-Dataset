@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Corretive_Action> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Effective_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Finished_Issue> .

<http://taxonomy.model/Implementing_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Improvement_Suggestion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/NC_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Non_Effective_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Finished_Issue> .

<http://taxonomy.model/Planned_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Preventive_Action> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Problem_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Finished_Issue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Issue> .

<http://taxonomy.model/Issue> a owl:Class .

