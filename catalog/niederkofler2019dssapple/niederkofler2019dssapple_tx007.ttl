@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/SpatialRegion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/SpatialEntity> .

<http://taxonomy.model/StorageFacility> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Building>,
        <http://taxonomy.model/SpatialEntity> .

<http://taxonomy.model/Building> a owl:Class .

<http://taxonomy.model/SpatialEntity> a owl:Class .

