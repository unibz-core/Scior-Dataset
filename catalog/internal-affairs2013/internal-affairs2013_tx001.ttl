@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/create> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/activity> .

<http://taxonomy.model/delete> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/activity> .

<http://taxonomy.model/search> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/activity> .

<http://taxonomy.model/update> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/activity> .

<http://taxonomy.model/activity> a owl:Class .

