@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:conclusion a owl:Class ;
    rdfs:subClassOf :sanction .

:penalty a owl:Class ;
    rdfs:subClassOf :sanction .

:reward a owl:Class ;
    rdfs:subClassOf :sanction .

:sanction a owl:Class .

