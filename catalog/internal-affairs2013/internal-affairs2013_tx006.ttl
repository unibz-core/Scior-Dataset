@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:civil a owl:Class ;
    rdfs:subClassOf :individual .

:civilServiceUnit a owl:Class ;
    rdfs:subClassOf :policeIAD .

:commandUnit a owl:Class ;
    rdfs:subClassOf :policeIAD .

:designatedRegistry a owl:Class ;
    rdfs:subClassOf :registry .

:designatedSection a owl:Class ;
    rdfs:subClassOf :section .

:generalInspector a owl:Class ;
    rdfs:subClassOf :military .

:registryResponsible a owl:Class ;
    rdfs:subClassOf :military .

:sectionResponsible a owl:Class ;
    rdfs:subClassOf :military .

:serviceUnit a owl:Class ;
    rdfs:subClassOf :policeIAD .

:viceInspector a owl:Class ;
    rdfs:subClassOf :military .

:Atomic a owl:Class ;
    rdfs:subClassOf :system .

:internalAffairDepartment a owl:Class ;
    rdfs:subClassOf :organization .

:organization a owl:Class ;
    rdfs:subClassOf :Composed .

:registry a owl:Class ;
    rdfs:subClassOf :organizationalPart .

:section a owl:Class ;
    rdfs:subClassOf :organizationalPart .

:Composed a owl:Class ;
    rdfs:subClassOf :system .

:individual a owl:Class ;
    rdfs:subClassOf :Atomic .

:organizationalPart a owl:Class ;
    rdfs:subClassOf :Composed .

:system a owl:Class .

:policeIAD a owl:Class ;
    rdfs:subClassOf :internalAffairDepartment .

:military a owl:Class ;
    rdfs:subClassOf :individual .

