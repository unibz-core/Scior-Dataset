@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/conclusion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/sanction> .

<http://taxonomy.model/penalty> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/sanction> .

<http://taxonomy.model/reward> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/sanction> .

<http://taxonomy.model/sanction> a owl:Class .

