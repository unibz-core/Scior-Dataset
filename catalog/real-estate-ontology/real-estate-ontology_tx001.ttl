@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Company_as_Lessor a owl:Class ;
    rdfs:subClassOf :Company_as_Owner,
        :Land_Lord_\(Lessor\) .

:Company_as_Tenant a owl:Class ;
    rdfs:subClassOf :Company,
        :Tenant_\(Lessee\) .

:Person_as_Lessor a owl:Class ;
    rdfs:subClassOf :Land_Lord_\(Lessor\),
        :Person_as_Owner .

:Person_as_Tenant a owl:Class ;
    rdfs:subClassOf :Person,
        :Tenant_\(Lessee\) .

:Company_as_Owner a owl:Class ;
    rdfs:subClassOf :Company,
        :Owner .

:Person_as_Owner a owl:Class ;
    rdfs:subClassOf :Owner,
        :Person .

:Company a owl:Class .

:Land_Lord_\(Lessor\) a owl:Class .

:Owner a owl:Class .

:Person a owl:Class .

:Tenant_\(Lessee\) a owl:Class .

