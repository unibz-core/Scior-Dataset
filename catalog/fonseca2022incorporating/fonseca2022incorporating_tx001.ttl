@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Captain a owl:Class ;
    rdfs:subClassOf :Person .

:Discoverer a owl:Class ;
    rdfs:subClassOf :Person .

:License_Holder a owl:Class ;
    rdfs:subClassOf :Agent .

:Organization a owl:Class ;
    rdfs:subClassOf :Agent .

:Painter a owl:Class ;
    rdfs:subClassOf :Author,
        :Person .

:Publisher a owl:Class ;
    rdfs:subClassOf :Agent .

:Sculptor a owl:Class ;
    rdfs:subClassOf :Author,
        :Person .

:Writer a owl:Class ;
    rdfs:subClassOf :Author,
        :Person .

:Author a owl:Class ;
    rdfs:subClassOf :Agent .

:Agent a owl:Class .

:Person a owl:Class ;
    rdfs:subClassOf :Agent .

