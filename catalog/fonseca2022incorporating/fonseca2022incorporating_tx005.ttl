@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Permanent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assignment> .

<http://taxonomy.model/Temporary> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assignment> .

<http://taxonomy.model/Assignment> a owl:Class .

