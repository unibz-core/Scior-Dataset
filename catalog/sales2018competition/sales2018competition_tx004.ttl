@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Direct_Competitor a owl:Class ;
    rdfs:subClassOf :Market_Competitor .

:Indirect_Competitor a owl:Class ;
    rdfs:subClassOf :Market_Competitor .

:Potential_Competitor a owl:Class ;
    rdfs:subClassOf :Economic_Agent .

:Supplier a owl:Class ;
    rdfs:subClassOf :Economic_Agent .

:Economic_Agent a owl:Class .

:Market_Competitor a owl:Class ;
    rdfs:subClassOf :Supplier .

