@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Direct_Competitor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Market_Competitor> .

<http://taxonomy.model/Indirect_Competitor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Market_Competitor> .

<http://taxonomy.model/Potential_Competitor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Economic_Agent> .

<http://taxonomy.model/Supplier> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Economic_Agent> .

<http://taxonomy.model/Economic_Agent> a owl:Class .

<http://taxonomy.model/Market_Competitor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Supplier> .

