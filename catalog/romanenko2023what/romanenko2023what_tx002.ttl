@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Librarian> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Employee> .

<http://taxonomy.model/Library> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Man> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Publisher> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Registered_Organization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Client>,
        <http://taxonomy.model/Organization> .

<http://taxonomy.model/Registered_Person> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Client>,
        <http://taxonomy.model/Person> .

<http://taxonomy.model/Translator> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Woman> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Writer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Employee> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Client> a owl:Class .

<http://taxonomy.model/Organization> a owl:Class .

<http://taxonomy.model/Person> a owl:Class .

