@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Librarian a owl:Class ;
    rdfs:subClassOf :Employee .

:Library a owl:Class ;
    rdfs:subClassOf :Organization .

:Man a owl:Class ;
    rdfs:subClassOf :Person .

:Publisher a owl:Class ;
    rdfs:subClassOf :Organization .

:Registered_Organization a owl:Class ;
    rdfs:subClassOf :Client,
        :Organization .

:Registered_Person a owl:Class ;
    rdfs:subClassOf :Client,
        :Person .

:Translator a owl:Class ;
    rdfs:subClassOf :Person .

:Woman a owl:Class ;
    rdfs:subClassOf :Person .

:Writer a owl:Class ;
    rdfs:subClassOf :Person .

:Employee a owl:Class ;
    rdfs:subClassOf :Person .

:Client a owl:Class .

:Organization a owl:Class .

:Person a owl:Class .

