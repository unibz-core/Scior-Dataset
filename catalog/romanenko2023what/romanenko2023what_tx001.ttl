@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Article a owl:Class ;
    rdfs:subClassOf :Document .

:Book a owl:Class ;
    rdfs:subClassOf :Document .

:Chapter a owl:Class ;
    rdfs:subClassOf :Document .

:Newspaper a owl:Class ;
    rdfs:subClassOf :Document .

:Object a owl:Class ;
    rdfs:subClassOf :Artifact .

:Patent a owl:Class ;
    rdfs:subClassOf :Document .

:Proceedings a owl:Class ;
    rdfs:subClassOf :Document .

:Sound_Recording a owl:Class ;
    rdfs:subClassOf :Recording .

:Video_Recording a owl:Class ;
    rdfs:subClassOf :Recording .

:Artifact a owl:Class .

:Recording a owl:Class ;
    rdfs:subClassOf :Work .

:Work a owl:Class ;
    rdfs:subClassOf :Artifact .

:Document a owl:Class ;
    rdfs:subClassOf :Work .

