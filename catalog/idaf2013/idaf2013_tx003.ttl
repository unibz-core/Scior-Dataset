@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Administrative_Structure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organizational_Structure> .

<http://taxonomy.model/Occupational_Structure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Structure> .

<http://taxonomy.model/Operational_Structure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organizational_Structure> .

<http://taxonomy.model/Organizational_Structure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Structure> .

<http://taxonomy.model/Structure> a owl:Class .

