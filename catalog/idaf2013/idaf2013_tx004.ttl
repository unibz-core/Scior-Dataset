@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Administrative_Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Department> .

<http://taxonomy.model/Advisory> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative_Organization> .

<http://taxonomy.model/Allocated_Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Department> .

<http://taxonomy.model/Allocated_Office> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Central_Office> .

<http://taxonomy.model/Allocation_Unit> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Board> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative_Organization> .

<http://taxonomy.model/Border_Station> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Station> .

<http://taxonomy.model/Diagnostic_Laboratory> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Operational_Organization> .

<http://taxonomy.model/Local_Office> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Office> .

<http://taxonomy.model/Mobile_Unit> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Operational_Organization> .

<http://taxonomy.model/Regional_Office> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Office> .

<http://taxonomy.model/Section> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative_Organization> .

<http://taxonomy.model/Service_Station> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Station> .

<http://taxonomy.model/Technical_Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Department> .

<http://taxonomy.model/UnidadeLotacao> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organizacao>,
        <http://taxonomy.model/Organization> .

<http://taxonomy.model/Central_Office> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Office> .

<http://taxonomy.model/Organizacao> a owl:Class .

<http://taxonomy.model/Station> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Operational_Organization> .

<http://taxonomy.model/Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative_Organization> .

<http://taxonomy.model/Office> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Operational_Organization> .

<http://taxonomy.model/Administrative_Organization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Operational_Organization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Organization> a owl:Class .

