@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Composition> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Relationship> .

<http://taxonomy.model/Contribution> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Relationship> .

<http://taxonomy.model/Dependency> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Relationship> .

<http://taxonomy.model/Relationship> a owl:Class .

