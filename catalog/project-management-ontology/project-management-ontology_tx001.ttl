@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Atomic> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Activity> .

<http://taxonomy.model/Composed> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Activity> .

<http://taxonomy.model/Performed_Activity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Activity> .

<http://taxonomy.model/Planned_Activity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Activity> .

<http://taxonomy.model/Activity> a owl:Class .

