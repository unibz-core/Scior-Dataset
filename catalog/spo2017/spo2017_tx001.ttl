@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Organization_Stakeholder a owl:Class ;
    rdfs:subClassOf :Stakeholder .

:Person_Stakeholder a owl:Class ;
    rdfs:subClassOf :Person,
        :Stakeholder .

:Project_Stakeholder a owl:Class ;
    rdfs:subClassOf :Stakeholder .

:Social_Agent a owl:Class ;
    rdfs:subClassOf :Agent .

:Team_Stakeholder a owl:Class ;
    rdfs:subClassOf :Stakeholder .

:Person a owl:Class ;
    rdfs:subClassOf :Physical_Agent .

:Physical_Agent a owl:Class ;
    rdfs:subClassOf :Agent .

:Agent a owl:Class .

:Stakeholder a owl:Class ;
    rdfs:subClassOf :Agent .

