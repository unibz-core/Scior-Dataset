@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Document_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact_Type> .

<http://taxonomy.model/Hardware_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object_Kind> .

<http://taxonomy.model/Software_Product_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact_Type> .

<http://taxonomy.model/Artifact_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object_Kind> .

<http://taxonomy.model/Object_Kind> a owl:Class .

