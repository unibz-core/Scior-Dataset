@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Claimed_Hired_Provider> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Hired_Service_Provider> .

<http://taxonomy.model/Committed_Service_Customer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Service_Customer> .

<http://taxonomy.model/Hired_Service_Provider> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Service_Provider_On_Negotiation> .

<http://taxonomy.model/Service_Customer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Target_Customer_On_Negotiation> .

<http://taxonomy.model/Service_Provider> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

<http://taxonomy.model/Service_Provider_On_Negotiation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Service_Provider> .

<http://taxonomy.model/Target_Customer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

<http://taxonomy.model/Target_Customer_On_Negotiation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Target_Customer> .

<http://taxonomy.model/Agent> a owl:Class .

