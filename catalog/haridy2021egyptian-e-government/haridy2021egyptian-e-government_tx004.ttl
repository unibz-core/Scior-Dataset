@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Bachelor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Scientific_Degree> .

<http://taxonomy.model/Masters> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Scientific_Degree> .

<http://taxonomy.model/PhD> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Scientific_Degree> .

<http://taxonomy.model/Scientific_Degree> a owl:Class .

