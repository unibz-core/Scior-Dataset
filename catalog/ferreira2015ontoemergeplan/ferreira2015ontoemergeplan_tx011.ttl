<http://taxonomy.model/General_OE_Process> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/OE_Process> .

<http://taxonomy.model/Mitigation_activity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/OE_Activity> .

<http://taxonomy.model/Organization_Holder_Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Preventive_activity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/OE_Activity> .

<http://taxonomy.model/Protect_Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention>,
        <http://taxonomy.model/Open_Commitment> .

<http://taxonomy.model/Specific_OE_Process> a owl:Class ;
