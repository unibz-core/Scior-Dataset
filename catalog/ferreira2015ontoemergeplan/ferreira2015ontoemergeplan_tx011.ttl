<http://taxonomy.model/Complex_Commitment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Commitment> .

<http://taxonomy.model/Open_Commitment> a owl:Class .

<http://taxonomy.model/OE_Process> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Commitment> a owl:Class .

<http://taxonomy.model/Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Commitment> .

<http://taxonomy.model/OE_Activity> a owl:Class ;
