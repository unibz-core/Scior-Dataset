@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Asset> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Derivative_Instrument>,
        <http://taxonomy.model/Economic_Resource> .

<http://taxonomy.model/Equity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Derivative_Instrument>,
        <http://taxonomy.model/Economic_Resource> .

<http://taxonomy.model/Liability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Derivative_Instrument>,
        <http://taxonomy.model/Economic_Resource> .

<http://taxonomy.model/Off_Balance> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Derivative_Instrument>,
        <http://taxonomy.model/Economic_Resource> .

<http://taxonomy.model/Derivative_Instrument> a owl:Class .

<http://taxonomy.model/Economic_Resource> a owl:Class .

