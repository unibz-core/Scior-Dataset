@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Asset a owl:Class ;
    rdfs:subClassOf :Derivative_Instrument,
        :Economic_Resource .

:Equity a owl:Class ;
    rdfs:subClassOf :Derivative_Instrument,
        :Economic_Resource .

:Liability a owl:Class ;
    rdfs:subClassOf :Derivative_Instrument,
        :Economic_Resource .

:Off_Balance a owl:Class ;
    rdfs:subClassOf :Derivative_Instrument,
        :Economic_Resource .

:Derivative_Instrument a owl:Class .

:Economic_Resource a owl:Class .

