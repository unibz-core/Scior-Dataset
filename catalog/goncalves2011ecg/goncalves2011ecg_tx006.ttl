@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:HeartAsBloodContainer a owl:Class ;
    rdfs:subClassOf :HumanHeart .

:HeartAsPump a owl:Class ;
    rdfs:subClassOf :HumanHeart .

:HumanBody a owl:Class ;
    rdfs:subClassOf :Chamber .

:LeftAtriumAsBloodContainer a owl:Class ;
    rdfs:subClassOf :LeftAtrium .

:LeftAtriumAsPump a owl:Class ;
    rdfs:subClassOf :LeftAtrium .

:LeftVentricleAsBloodContainer a owl:Class ;
    rdfs:subClassOf :LeftVentricle .

:LeftVentricleAsPump a owl:Class ;
    rdfs:subClassOf :LeftVentricle .

:RightAtriumAsBloodContainer a owl:Class ;
    rdfs:subClassOf :RightAtrium .

:RightAtriumAsPump a owl:Class ;
    rdfs:subClassOf :RightAtrium .

:RightVentricleAsBloodContainer a owl:Class ;
    rdfs:subClassOf :RightVentricle .

:RightVentricleAsPump a owl:Class ;
    rdfs:subClassOf :RightVentricle .

:HumanHeart a owl:Class ;
    rdfs:subClassOf :Chamber,
        :Muscle .

:LeftAtrium a owl:Class ;
    rdfs:subClassOf :Chamber,
        :Muscle .

:LeftVentricle a owl:Class ;
    rdfs:subClassOf :Chamber,
        :Muscle .

:RightAtrium a owl:Class ;
    rdfs:subClassOf :Chamber,
        :Muscle .

:RightVentricle a owl:Class ;
    rdfs:subClassOf :Chamber,
        :Muscle .

:Muscle a owl:Class .

:Chamber a owl:Class .

