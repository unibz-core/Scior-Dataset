@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/HeartAsBloodContainer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/HumanHeart> .

<http://taxonomy.model/HeartAsPump> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/HumanHeart> .

<http://taxonomy.model/HumanBody> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber> .

<http://taxonomy.model/LeftAtriumAsBloodContainer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LeftAtrium> .

<http://taxonomy.model/LeftAtriumAsPump> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LeftAtrium> .

<http://taxonomy.model/LeftVentricleAsBloodContainer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LeftVentricle> .

<http://taxonomy.model/LeftVentricleAsPump> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LeftVentricle> .

<http://taxonomy.model/RightAtriumAsBloodContainer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RightAtrium> .

<http://taxonomy.model/RightAtriumAsPump> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RightAtrium> .

<http://taxonomy.model/RightVentricleAsBloodContainer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RightVentricle> .

<http://taxonomy.model/RightVentricleAsPump> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RightVentricle> .

<http://taxonomy.model/HumanHeart> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber>,
        <http://taxonomy.model/Muscle> .

<http://taxonomy.model/LeftAtrium> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber>,
        <http://taxonomy.model/Muscle> .

<http://taxonomy.model/LeftVentricle> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber>,
        <http://taxonomy.model/Muscle> .

<http://taxonomy.model/RightAtrium> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber>,
        <http://taxonomy.model/Muscle> .

<http://taxonomy.model/RightVentricle> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Chamber>,
        <http://taxonomy.model/Muscle> .

<http://taxonomy.model/Muscle> a owl:Class .

<http://taxonomy.model/Chamber> a owl:Class .

