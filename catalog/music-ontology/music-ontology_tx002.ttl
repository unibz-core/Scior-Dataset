@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Music_Composer a owl:Class ;
    rdfs:subClassOf :Composer .

:Musician a owl:Class ;
    rdfs:subClassOf :Individual_Artist,
        :Musical_Member .

:Orchestra a owl:Class ;
    rdfs:subClassOf :Musical_Group .

:Publisher a owl:Class ;
    rdfs:subClassOf :Person .

:Software_Program a owl:Class ;
    rdfs:subClassOf :Artist .

:Artistic_Group a owl:Class ;
    rdfs:subClassOf :Artist,
        :Group .

:Composer a owl:Class ;
    rdfs:subClassOf :Artist .

:Group a owl:Class .

:Individual_Artist a owl:Class ;
    rdfs:subClassOf :Artist,
        :Artistic_Member,
        :Person .

:Member a owl:Class ;
    rdfs:subClassOf :Person .

:Musical_Group a owl:Class ;
    rdfs:subClassOf :Artistic_Group .

:Musical_Member a owl:Class ;
    rdfs:subClassOf :Artistic_Member .

:Artistic_Member a owl:Class ;
    rdfs:subClassOf :Member .

:Person a owl:Class .

:Artist a owl:Class .

