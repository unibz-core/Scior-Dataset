@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Ballet> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Music_Work> .

<http://taxonomy.model/Opera> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Music_Work> .

<http://taxonomy.model/Overture> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Orchestral_Work> .

<http://taxonomy.model/Suite> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Music_Work> .

<http://taxonomy.model/Symphonic_Poem> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Orchestral_Work> .

<http://taxonomy.model/Symphony> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Orchestral_Work> .

<http://taxonomy.model/Art_Work> a owl:Class .

<http://taxonomy.model/Orchestral_Work> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Music_Work> .

<http://taxonomy.model/Music_Work> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Art_Work> .

