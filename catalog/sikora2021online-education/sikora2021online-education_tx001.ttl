@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Book a owl:Class ;
    rdfs:subClassOf :Resource .

:Collection a owl:Class ;
    rdfs:subClassOf :Creatable,
        :Deletable,
        :Editable,
        :Resource .

:Comment a owl:Class ;
    rdfs:subClassOf :Commentable,
        :Creatable,
        :Deletable,
        :Editable,
        :Interaction,
        :Upvotable .

:Corporate_Resource a owl:Class ;
    rdfs:subClassOf :Resource .

:Course a owl:Class ;
    rdfs:subClassOf :Resource .

:Creation a owl:Class ;
    rdfs:subClassOf :Interaction .

:Delete a owl:Class ;
    rdfs:subClassOf :Interaction .

:Edit a owl:Class ;
    rdfs:subClassOf :Interaction .

:Follow a owl:Class ;
    rdfs:subClassOf :Interaction .

:Learning_Path a owl:Class ;
    rdfs:subClassOf :Creatable,
        :Deletable,
        :Editable,
        :Resource .

:Link a owl:Class ;
    rdfs:subClassOf :Resource .

:Podcast a owl:Class ;
    rdfs:subClassOf :Resource .

:Private_Interaction a owl:Class ;
    rdfs:subClassOf :Interaction .

:Public_Interaction a owl:Class ;
    rdfs:subClassOf :Interaction .

:Public_Resource a owl:Class ;
    rdfs:subClassOf :Resource .

:Search a owl:Class ;
    rdfs:subClassOf :Interaction .

:Upvote a owl:Class ;
    rdfs:subClassOf :Interaction .

:Video a owl:Class ;
    rdfs:subClassOf :Resource .

:View a owl:Class ;
    rdfs:subClassOf :Interaction .

:Viewable a owl:Class .

:Commentable a owl:Class .

:Upvotable a owl:Class .

:Creatable a owl:Class .

:Deletable a owl:Class .

:Editable a owl:Class .

:Resource a owl:Class ;
    rdfs:subClassOf :Commentable,
        :Upvotable,
        :Viewable .

:Interaction a owl:Class .

