@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Buyer a owl:Class ;
    rdfs:subClassOf :Brokerage .

:Hired_Brokerage a owl:Class ;
    rdfs:subClassOf :Brokerage .

:Public_Traded_Company a owl:Class ;
    rdfs:subClassOf :Organization .

:Seller a owl:Class ;
    rdfs:subClassOf :Brokerage .

:Stock_Keeper a owl:Class ;
    rdfs:subClassOf :Stock_Exchange .

:StockholderOrganization a owl:Class ;
    rdfs:subClassOf :Organization,
        :StockHolder .

:Stockholder_Person a owl:Class ;
    rdfs:subClassOf :Person,
        :StockHolder .

:Person a owl:Class .

:Stock_Exchange a owl:Class ;
    rdfs:subClassOf :Organization .

:StockHolder a owl:Class .

:Brokerage a owl:Class ;
    rdfs:subClassOf :Organization .

:Organization a owl:Class .

