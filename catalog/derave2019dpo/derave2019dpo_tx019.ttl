@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Booking a owl:Class ;
    rdfs:subClassOf :Transaction .

:Centralized_Transaction a owl:Class ;
    rdfs:subClassOf :Transaction .

:Decentralized_Transaction a owl:Class ;
    rdfs:subClassOf :Transaction .

:Immediate_Acces_Transaction a owl:Class ;
    rdfs:subClassOf :Transaction .

:Investment a owl:Class ;
    rdfs:subClassOf :MS_User_Affiliation .

:Quoted_Transaction a owl:Class ;
    rdfs:subClassOf :Transaction .

:Registration a owl:Class ;
    rdfs:subClassOf :MS_User_Affiliation,
        :One-Sided_Platform_User_Affiliation,
        :User_Affiliation_Agreement .

:Subscription a owl:Class ;
    rdfs:subClassOf :MS_User_Affiliation,
        :One-Sided_Platform_User_Affiliation,
        :User_Affiliation_Agreement .

:One-Sided_Platform_User_Affiliation a owl:Class ;
    rdfs:subClassOf :User_Affiliation_Agreement .

:MS_User_Affiliation a owl:Class ;
    rdfs:subClassOf :User_Affiliation_Agreement .

:Transaction a owl:Class ;
    rdfs:subClassOf :MS_User_Affiliation,
        :User_Affiliation_Agreement .

:User_Affiliation_Agreement a owl:Class .

