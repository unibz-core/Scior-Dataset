@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Assumption> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Proposition> .

<http://taxonomy.model/FUnctional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/Functional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/GORO::Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RSRO::Requirements> .

<http://taxonomy.model/Hardgoal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Goal> .

<http://taxonomy.model/Product_Quality_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Non-functional_Requirement> .

<http://taxonomy.model/RSRO::Assumption> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Proposition> .

<http://taxonomy.model/RSRO::Functional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RSRO::Requirements> .

<http://taxonomy.model/RSRO::Non-Functional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/RSRO::Requirements> .

<http://taxonomy.model/Softgoal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Goal> .

<http://taxonomy.model/Non-functional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/RSRO::Requirements> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Goal> .

<http://taxonomy.model/Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Goal> .

<http://taxonomy.model/UFO-C::Proposition> a owl:Class .

<http://taxonomy.model/UFO-C::Goal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-C::Proposition> .

