@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Provider> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Economic_Agent> .

<http://taxonomy.model/Receiver> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Economic_Agent> .

<http://taxonomy.model/Agent> a owl:Class .

<http://taxonomy.model/Economic_Agent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

