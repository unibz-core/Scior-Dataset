@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Functional_Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Goal> .

<http://taxonomy.model/Gradable_NFR> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/NFR_(Quality_Goal)> .

<http://taxonomy.model/Hardgoal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Goal> .

<http://taxonomy.model/Quality_Constraint> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/NFR_(Quality_Goal)> .

<http://taxonomy.model/Softgoal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Goal> .

<http://taxonomy.model/Vague_NFR> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/NFR_(Quality_Goal)> .

<http://taxonomy.model/NFR_(Quality_Goal)> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Goal> .

<http://taxonomy.model/Goal> a owl:Class .

