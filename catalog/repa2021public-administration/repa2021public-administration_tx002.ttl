@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Child> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/Company> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/PA_Client> .

<http://taxonomy.model/Employed> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/In_requalification> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Unemployed> .

<http://taxonomy.model/Pupil> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/Seeking_for_employment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Unemployed> .

<http://taxonomy.model/Senior> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/PA_Client> a owl:Class .

<http://taxonomy.model/Unemployed> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/Citizen> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/PA_Client> .

