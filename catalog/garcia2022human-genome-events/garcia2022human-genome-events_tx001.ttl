@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/active_entity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/biological_entity> .

<http://taxonomy.model/degraded_entity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/biological_entity> .

<http://taxonomy.model/input> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity_in_process> .

<http://taxonomy.model/output> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity_in_process> .

<http://taxonomy.model/regulator> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity_in_process> .

<http://taxonomy.model/Entity_in_process> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/biological_entity> .

<http://taxonomy.model/biological_entity> a owl:Class .

