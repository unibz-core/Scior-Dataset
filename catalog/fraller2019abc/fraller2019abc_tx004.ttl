@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Asset_Account> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/General_Ledger_Account> .

<http://taxonomy.model/Equity_Account> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/General_Ledger_Account> .

<http://taxonomy.model/Expense_Account> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/General_Ledger_Account> .

<http://taxonomy.model/Liability_Account> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/General_Ledger_Account> .

<http://taxonomy.model/Revenue_Account> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/General_Ledger_Account> .

<http://taxonomy.model/General_Ledger_Account> a owl:Class .

