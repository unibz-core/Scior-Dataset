@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/CommunicativeIntention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CommunicativeCommitment> .

<http://taxonomy.model/InterpretedIntention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CommunicativeCommitment> .

<http://taxonomy.model/IntentionInternalCommitment> a owl:Class .

<http://taxonomy.model/CommunicativeCommitment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/IntentionInternalCommitment> .

