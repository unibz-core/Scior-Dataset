@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Audio> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Attachment> .

<http://taxonomy.model/Image> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Attachment> .

<http://taxonomy.model/Video> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Attachment> .

<http://taxonomy.model/Attachment> a owl:Class .

