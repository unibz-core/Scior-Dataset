@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ActiveProfessor a owl:Class ;
    rdfs:subClassOf :Professor .

:CourseCoordinator a owl:Class ;
    rdfs:subClassOf :Professor .

:DepartmentChief a owl:Class ;
    rdfs:subClassOf :Professor .

:EgressStudent a owl:Class ;
    rdfs:subClassOf :Student .

:InactiveProfessor a owl:Class ;
    rdfs:subClassOf :Professor .

:RegularStudent a owl:Class ;
    rdfs:subClassOf :Student .

:SuspendedStudent a owl:Class ;
    rdfs:subClassOf :Student .

:Person a owl:Class .

:Student a owl:Class ;
    rdfs:subClassOf :Person .

:Professor a owl:Class ;
    rdfs:subClassOf :Person .

