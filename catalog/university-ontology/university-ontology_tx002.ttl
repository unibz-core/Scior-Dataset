@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Center> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Higher_Education_Institution> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Organization> a owl:Class .

