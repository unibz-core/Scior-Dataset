@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Consumable> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/Equipment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/Etc> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/NonPlayerCharacter> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity> .

<http://taxonomy.model/Entity> a owl:Class .

<http://taxonomy.model/Object> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity> .

