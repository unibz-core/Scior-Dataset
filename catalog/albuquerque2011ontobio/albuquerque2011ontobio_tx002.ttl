@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/FirstQuarterMoon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MoonPhase> .

<http://taxonomy.model/FullMoon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MoonPhase> .

<http://taxonomy.model/LastQuarterMoon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MoonPhase> .

<http://taxonomy.model/NewMoon> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MoonPhase> .

<http://taxonomy.model/MoonPhase> a owl:Class .

