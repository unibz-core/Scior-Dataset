@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Cloudy> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/WeatherCondition> .

<http://taxonomy.model/Rainy> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/WeatherCondition> .

<http://taxonomy.model/Sunny> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/WeatherCondition> .

<http://taxonomy.model/WeatherCondition> a owl:Class .

