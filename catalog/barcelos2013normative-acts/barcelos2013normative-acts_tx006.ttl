@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Chapter_composed_by_Article a owl:Class ;
    rdfs:subClassOf :Chapter .

:Chapter_composed_by_Section a owl:Class ;
    rdfs:subClassOf :Chapter .

:Component_Chapter a owl:Class ;
    rdfs:subClassOf :Chapter .

:Chapter a owl:Class .

