@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Component_Section> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Section> .

<http://taxonomy.model/Composed_Section> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Section> .

<http://taxonomy.model/Subsection> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Simple_Section> .

<http://taxonomy.model/Simple_Section> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Section> .

<http://taxonomy.model/Section> a owl:Class .

