@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:EducationalInstitution a owl:Class ;
    rdfs:subClassOf :Organization .

:ParticipantWithQuestion a owl:Class ;
    rdfs:subClassOf :Participant .

:ParticipantWithanswer a owl:Class ;
    rdfs:subClassOf :Participant .

:Professor a owl:Class ;
    rdfs:subClassOf :Participant .

:Tutor a owl:Class ;
    rdfs:subClassOf :ActiveStudent .

:ActiveStudent a owl:Class ;
    rdfs:subClassOf :Student .

:Organization a owl:Class ;
    rdfs:subClassOf :Agent .

:Person a owl:Class ;
    rdfs:subClassOf :Agent .

:Student a owl:Class ;
    rdfs:subClassOf :Participant .

:Agent a owl:Class .

:Participant a owl:Class ;
    rdfs:subClassOf :Person .

