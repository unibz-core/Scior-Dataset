@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/AnsweredQuestion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Question> .

<http://taxonomy.model/CharacterizedQuestion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Question> .

<http://taxonomy.model/NonCharacterizedQuestion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Question> .

<http://taxonomy.model/OpenQuestion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Question> .

<http://taxonomy.model/Question> a owl:Class .

