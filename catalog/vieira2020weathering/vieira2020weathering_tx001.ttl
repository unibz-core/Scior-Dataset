@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Earth_Solid a owl:Class ;
    rdfs:subClassOf :Earth_Material .

:Meteoric_Water a owl:Class ;
    rdfs:subClassOf :Water .

:Pore_Water a owl:Class ;
    rdfs:subClassOf :Pore_Fluid,
        :Water .

:Pore_Fluid a owl:Class ;
    rdfs:subClassOf :Earth_Fluid .

:Earth_Fluid a owl:Class ;
    rdfs:subClassOf :Earth_Material .

:Earth_Material a owl:Class .

:Water a owl:Class ;
    rdfs:subClassOf :Earth_Fluid .

