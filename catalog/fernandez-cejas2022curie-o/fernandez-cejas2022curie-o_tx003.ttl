@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Customer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/Lead> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/Loyal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/Promoter> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/Prospect> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/Stranger> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CustomerRelationshipStatus> .

<http://taxonomy.model/CustomerRelationshipStatus> a owl:Class .

