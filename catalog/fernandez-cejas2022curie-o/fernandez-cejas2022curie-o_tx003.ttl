@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Customer a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:Lead a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:Loyal a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:Promoter a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:Prospect a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:Stranger a owl:Class ;
    rdfs:subClassOf :CustomerRelationshipStatus .

:CustomerRelationshipStatus a owl:Class .

