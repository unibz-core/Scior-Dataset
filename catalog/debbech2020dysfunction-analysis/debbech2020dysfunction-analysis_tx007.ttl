@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Fault_Emergency_Failure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Failure> .

<http://taxonomy.model/Ill-intentional_Stakeholder-caused_Failure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Stakeholder-caused_Failure> .

<http://taxonomy.model/Non-intentional_Stakeholder-caused_Failure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Stakeholder-caused_Failure> .

<http://taxonomy.model/Event> a owl:Class .

<http://taxonomy.model/Failure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Event> .

<http://taxonomy.model/Stakeholder-caused_Failure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Failure> .

