@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Environment_Object_Fault> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Fault> .

<http://taxonomy.model/Hazardous_Usage> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exposure> .

<http://taxonomy.model/System_Equipment_Fault> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Fault> .

<http://taxonomy.model/Defect> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exposure> .

<http://taxonomy.model/Disposition> a owl:Class .

<http://taxonomy.model/Exposure> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disposition> .

<http://taxonomy.model/Fault> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Defect> .

