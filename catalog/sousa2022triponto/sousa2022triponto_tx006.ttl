@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Boat> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transportation_Mean> .

<http://taxonomy.model/Bus> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transportation_Mean> .

<http://taxonomy.model/Car> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transportation_Mean> .

<http://taxonomy.model/Plane> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transportation_Mean> .

<http://taxonomy.model/Train> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transportation_Mean> .

<http://taxonomy.model/Transportation_Mean> a owl:Class .

