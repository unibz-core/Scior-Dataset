@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Child a owl:Class ;
    rdfs:subClassOf :Person .

:Costumer a owl:Class ;
    rdfs:subClassOf :Adult .

:Driver a owl:Class ;
    rdfs:subClassOf :Adult .

:International_Tourist a owl:Class ;
    rdfs:subClassOf :Tourist .

:National_Tourist a owl:Class ;
    rdfs:subClassOf :Tourist .

:Passenger a owl:Class ;
    rdfs:subClassOf :Person .

:Adult a owl:Class ;
    rdfs:subClassOf :Person .

:Tourist a owl:Class ;
    rdfs:subClassOf :Person .

:Person a owl:Class .

