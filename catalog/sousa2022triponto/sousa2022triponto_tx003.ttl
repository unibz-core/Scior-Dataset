@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Child> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Costumer> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Adult> .

<http://taxonomy.model/Driver> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Adult> .

<http://taxonomy.model/International_Tourist> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Tourist> .

<http://taxonomy.model/National_Tourist> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Tourist> .

<http://taxonomy.model/Passenger> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Adult> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Tourist> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Person> a owl:Class .

