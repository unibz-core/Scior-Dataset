@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Action_Card a owl:Class ;
    rdfs:subClassOf :Resource .

:Action_Points a owl:Class ;
    rdfs:subClassOf :Resource .

:Areas a owl:Class ;
    rdfs:subClassOf :Component .

:Card a owl:Class ;
    rdfs:subClassOf :Token .

:Die_Marker a owl:Class ;
    rdfs:subClassOf :Die .

:Modular_Board a owl:Class ;
    rdfs:subClassOf :Component .

:Paper_And_Pencil a owl:Class ;
    rdfs:subClassOf :Component .

:Pattern a owl:Class ;
    rdfs:subClassOf :Component .

:Tile a owl:Class ;
    rdfs:subClassOf :Token .

:Victory_Points a owl:Class ;
    rdfs:subClassOf :Resource .

:Die a owl:Class ;
    rdfs:subClassOf :Component .

:Data_Representation a owl:Class .

:Token a owl:Class ;
    rdfs:subClassOf :Component .

:Resource a owl:Class ;
    rdfs:subClassOf :Data_Representation .

:Component a owl:Class ;
    rdfs:subClassOf :Data_Representation .

