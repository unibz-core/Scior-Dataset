@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Anxiety a owl:Class ;
    rdfs:subClassOf :Fear .

:Desperation a owl:Class ;
    rdfs:subClassOf :Fear .

:Dread a owl:Class ;
    rdfs:subClassOf :Fear .

:Horror a owl:Class ;
    rdfs:subClassOf :Fear .

:Nervousness a owl:Class ;
    rdfs:subClassOf :Fear .

:Panic a owl:Class ;
    rdfs:subClassOf :Fear .

:Terror a owl:Class ;
    rdfs:subClassOf :Fear .

:Trepidation a owl:Class ;
    rdfs:subClassOf :Fear .

:Fear a owl:Class .

