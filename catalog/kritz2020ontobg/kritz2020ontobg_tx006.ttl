@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Abhorrence> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Aversion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Dislike> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Distaste> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Loathing> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Repugnance> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Revulsion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disgust> .

<http://taxonomy.model/Disgust> a owl:Class .

