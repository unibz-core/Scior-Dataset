@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Annoyance> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Argumentativeness> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Bitterness> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Exasperation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Frustration> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Fury> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Vengefulness> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Anger> .

<http://taxonomy.model/Anger> a owl:Class .

