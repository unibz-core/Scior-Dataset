@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ActiveOrganization a owl:Class ;
    rdfs:subClassOf :ActiveAgent,
        :Organization .

:Band a owl:Class ;
    rdfs:subClassOf :Organization,
        :Performer_Artist .

:Band_Member a owl:Class ;
    rdfs:subClassOf :Individual_Artist .

:Deceased_Person a owl:Class ;
    rdfs:subClassOf :Person .

:ExtinctOrganization a owl:Class ;
    rdfs:subClassOf :Organization .

:Father a owl:Class ;
    rdfs:subClassOf :Male_Person,
        :Parent .

:Living_Person a owl:Class ;
    rdfs:subClassOf :ActiveAgent,
        :Person .

:Mother a owl:Class ;
    rdfs:subClassOf :Female_Person,
        :Parent .

:Offspring a owl:Class ;
    rdfs:subClassOf :Person .

:Female_Person a owl:Class ;
    rdfs:subClassOf :Person .

:Individual_Artist a owl:Class ;
    rdfs:subClassOf :Performer_Artist,
        :Person .

:Male_Person a owl:Class ;
    rdfs:subClassOf :Person .

:ActiveAgent a owl:Class ;
    rdfs:subClassOf :Agent .

:Parent a owl:Class ;
    rdfs:subClassOf :Person .

:Performer_Artist a owl:Class .

:Agent a owl:Class .

:Organization a owl:Class ;
    rdfs:subClassOf :Agent .

:Person a owl:Class ;
    rdfs:subClassOf :Agent .

