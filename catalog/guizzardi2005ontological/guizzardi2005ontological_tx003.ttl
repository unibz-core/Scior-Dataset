@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Human_Heart> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Organ> .

<http://taxonomy.model/Statue> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Inanimate_Entity> .

<http://taxonomy.model/Biological_Entity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Material_Entity> .

<http://taxonomy.model/Human_Organ> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Biological_Entity> .

<http://taxonomy.model/Inanimate_Entity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Material_Entity> .

<http://taxonomy.model/Material_Entity> a owl:Class .

