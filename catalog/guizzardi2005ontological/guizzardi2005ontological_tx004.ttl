@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Belief> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MentalState> .

<http://taxonomy.model/Desire> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MentalState> .

<http://taxonomy.model/Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/MentalState> .

<http://taxonomy.model/MentalState> a owl:Class .

