@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Human_Heart a owl:Class ;
    rdfs:subClassOf :Human_Organ .

:Statue a owl:Class ;
    rdfs:subClassOf :Inanimate_Entity .

:Biological_Entity a owl:Class ;
    rdfs:subClassOf :Material_Entity .

:Human_Organ a owl:Class ;
    rdfs:subClassOf :Biological_Entity .

:Inanimate_Entity a owl:Class ;
    rdfs:subClassOf :Material_Entity .

:Material_Entity a owl:Class .

