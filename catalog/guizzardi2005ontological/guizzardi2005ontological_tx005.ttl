@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Gallery> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/GeographicalSpace> .

<http://taxonomy.model/Museum> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/GeographicalSpace> .

<http://taxonomy.model/SurgeryRoom> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/GeographicalSpace> .

<http://taxonomy.model/SpatialThing> a owl:Class .

<http://taxonomy.model/GeographicalSpace> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/SpatialThing> .

