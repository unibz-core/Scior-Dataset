@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Benefit a owl:Class ;
    rdfs:subClassOf :Rational_Value_Component .

:Gain a owl:Class ;
    rdfs:subClassOf :Psychological_Value,
        :Psychological_Value_Component .

:Loss a owl:Class ;
    rdfs:subClassOf :Psychological_Value,
        :Psychological_Value_Component .

:Rational_Value a owl:Class ;
    rdfs:subClassOf :Value .

:Sacrifice a owl:Class ;
    rdfs:subClassOf :Rational_Value_Component .

:Psychological_Value a owl:Class ;
    rdfs:subClassOf :Value .

:Psychological_Value_Component a owl:Class ;
    rdfs:subClassOf :Value_Component .

:Rational_Value_Component a owl:Class ;
    rdfs:subClassOf :Value_Component .

:Value a owl:Class .

:Value_Component a owl:Class .

