@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Decision> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Intuition> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Moment> .

<http://taxonomy.model/Loss_Aversion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Cognitive_Bias>,
        <http://taxonomy.model/Desire> .

<http://taxonomy.model/Cognitive_Bias> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Moment> .

<http://taxonomy.model/Desire> a owl:Class .

<http://taxonomy.model/Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Moment> .

<http://taxonomy.model/Mental_Moment> a owl:Class .

