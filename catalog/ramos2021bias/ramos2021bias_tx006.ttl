@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Reference_Point> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Belief> .

<http://taxonomy.model/Risk_Averse_Preference> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Preference> .

<http://taxonomy.model/Risk_Seeking_Preference> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Preference> .

<http://taxonomy.model/Belief> a owl:Class .

<http://taxonomy.model/Preference> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Belief> .

