@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Causal_Likelihood a owl:Class ;
    rdfs:subClassOf :Likelihood .

:Intention a owl:Class ;
    rdfs:subClassOf :Mental_Aspect .

:Risk a owl:Class ;
    rdfs:subClassOf :Value .

:Threat_Capability a owl:Class ;
    rdfs:subClassOf :Capability .

:Triggering_Likelihood a owl:Class ;
    rdfs:subClassOf :Likelihood .

:Vulnerability a owl:Class ;
    rdfs:subClassOf :Disposition .

:Capability a owl:Class ;
    rdfs:subClassOf :Disposition .

:Mental_Aspect a owl:Class ;
    rdfs:subClassOf :Mode .

:Value a owl:Class ;
    rdfs:subClassOf :Quality .

:Disposition a owl:Class ;
    rdfs:subClassOf :Mode .

:Likelihood a owl:Class ;
    rdfs:subClassOf :Quality .

:Mode a owl:Class ;
    rdfs:subClassOf :Trope .

:Quality a owl:Class ;
    rdfs:subClassOf :Trope .

:Trope a owl:Class .

