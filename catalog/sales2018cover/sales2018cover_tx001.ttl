@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Object a owl:Class ;
    rdfs:subClassOf :Substantial .

:Object_at_Risk a owl:Class ;
    rdfs:subClassOf :Value_Object .

:Risk_Assessor a owl:Class ;
    rdfs:subClassOf :Value_Assessor .

:Risk_Enabler a owl:Class ;
    rdfs:subClassOf :Value_Enabler .

:Risk_Subject a owl:Class ;
    rdfs:subClassOf :Value_Subject .

:Threat_Agent a owl:Class ;
    rdfs:subClassOf :Agent,
        :Threat_Object .

:Threat_Object a owl:Class ;
    rdfs:subClassOf :Value_Object .

:Value_Assessor a owl:Class ;
    rdfs:subClassOf :Agent .

:Value_Enabler a owl:Class ;
    rdfs:subClassOf :Endurant .

:Value_Subject a owl:Class ;
    rdfs:subClassOf :Agent .

:Substantial a owl:Class ;
    rdfs:subClassOf :Endurant .

:Value_Object a owl:Class ;
    rdfs:subClassOf :Endurant .

:Agent a owl:Class ;
    rdfs:subClassOf :Substantial .

:Endurant a owl:Class .

