@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Experience_Risk_Assessment a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Experience_Value_Asc.>,
        :Risk_Assessment .

:Gain_Ascription a owl:Class ;
    rdfs:subClassOf :Value_Asc._Component .

:Loss_Ascription a owl:Class ;
    rdfs:subClassOf :Value_Asc._Component .

<http://taxonomy.model/Object_Risk_Assess.> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object_Value_Asc.>,
        :Risk_Assessment .

:Value_Asc._Ex_Post a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

:Value_Assertion a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

:Value_Perception a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

<http://taxonomy.model/Experience_Value_Asc.> a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

<http://taxonomy.model/Object_Value_Asc.> a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

:Value_Asc._Ex_Ante a owl:Class ;
    rdfs:subClassOf :Value_Ascription .

:Relator a owl:Class .

:Risk_Assessment a owl:Class ;
    rdfs:subClassOf :Value_Asc._Ex_Ante .

:Value_Asc._Component a owl:Class ;
    rdfs:subClassOf :Relator .

:Value_Ascription a owl:Class ;
    rdfs:subClassOf :Relator .

