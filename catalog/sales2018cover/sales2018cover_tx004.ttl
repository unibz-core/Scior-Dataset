@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Causal_Likelihood> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Likelihood> .

<http://taxonomy.model/Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Aspect> .

<http://taxonomy.model/Risk> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Value> .

<http://taxonomy.model/Threat_Capability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Capability> .

<http://taxonomy.model/Triggering_Likelihood> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Likelihood> .

<http://taxonomy.model/Vulnerability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disposition> .

<http://taxonomy.model/Capability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disposition> .

<http://taxonomy.model/Mental_Aspect> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mode> .

<http://taxonomy.model/Value> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Quality> .

<http://taxonomy.model/Disposition> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mode> .

<http://taxonomy.model/Likelihood> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Quality> .

<http://taxonomy.model/Mode> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Trope> .

<http://taxonomy.model/Quality> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Trope> .

<http://taxonomy.model/Trope> a owl:Class .

