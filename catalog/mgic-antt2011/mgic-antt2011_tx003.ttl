@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Advento_do_Termo_Contratual> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Não_Indenizável> .

<http://taxonomy.model/Advertência> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Anulação> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Anulação_do_Contrato> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Indenizável> .

<http://taxonomy.model/Caducidade> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Indenizável> .

<http://taxonomy.model/Cassação> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Declaração_de_Caducidade> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Declaração_de_Inidoneidade> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Encampação> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Indenizável>,
        <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Falência_da_Concessionária> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Não_Indenizável> .

<http://taxonomy.model/Intervenção> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Multa> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Penalidade_Aplicada> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Penalidade_Prevista> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Rescisão> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Rescisão_do_Contrato> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_da_Concessão_Indenizável> .

<http://taxonomy.model/Suspensão> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Penalidade> .

<http://taxonomy.model/Extinção_da_Concessão_Não_Indenizável> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_de_Concessão> .

<http://taxonomy.model/Extinção_de_Concessão> a owl:Class .

<http://taxonomy.model/Extinção_da_Concessão_Indenizável> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extinção_de_Concessão> .

<http://taxonomy.model/Penalidade> a owl:Class .

