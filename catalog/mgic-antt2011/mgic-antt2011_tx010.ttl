@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Atividade_Extraordinária> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade_Operacional> .

<http://taxonomy.model/Atividade_Financeira> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade_Não_Operacional> .

<http://taxonomy.model/Atividade_Ordinária> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade_Operacional> .

<http://taxonomy.model/Exploração_Utilidades> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_Faixa_de_Domínio> .

<http://taxonomy.model/Exploração_de_Acesso> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_FD_com_Projeto_de_Engenharia>,
        <http://taxonomy.model/Exploração_de_Faixa_de_Domínio_sem_Receita> .

<http://taxonomy.model/Exploração_de_Faixa_de_Domínio_Especial> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_Faixa_de_Domínio> .

<http://taxonomy.model/Exploração_de_Publicidade> a owl:Class ;
