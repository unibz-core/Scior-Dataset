<http://taxonomy.model/Pagamento_de_Pedágio> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Ordinária> .

<http://taxonomy.model/Receita_Acessória> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Receita_Alternativa> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Receita_Complementar> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Receita_de_Aplicações_Financeiras> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Fonte_de_Receita_Rodoviária> .

<http://taxonomy.model/Receita_de_Convênio> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Receita_de_Projeto_Associado> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Venda_de_Ativo> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade_Não_Operacional>,
        <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Venda_de_Sucata> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade_Não_Operacional>,
        <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Exploração_da_Faixa_de_Domínio_com_Receita> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_Faixa_de_Domínio>,
        <http://taxonomy.model/Receita_Rodoviária_Extraordinária> .

<http://taxonomy.model/Exploração_de_FD_com_Projeto_de_Engenharia> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_Faixa_de_Domínio> .

<http://taxonomy.model/Exploração_de_Faixa_de_Domínio_sem_Receita> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Exploração_de_Faixa_de_Domínio> .

<http://taxonomy.model/Receita_Rodoviária_Ordinária> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Fonte_de_Receita_Rodoviária> .

<http://taxonomy.model/Atividade> a owl:Class .

<http://taxonomy.model/Atividade_Operacional> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade> .

<http://taxonomy.model/Atividade_Não_Operacional> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Atividade> .

<http://taxonomy.model/Fonte_de_Receita_Rodoviária> a owl:Class .

<http://taxonomy.model/Exploração_de_Faixa_de_Domínio> a owl:Class .

<http://taxonomy.model/Receita_Rodoviária_Extraordinária> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Fonte_de_Receita_Rodoviária> .

