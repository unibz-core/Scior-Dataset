@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Abstract_Class a owl:Class ;
    rdfs:subClassOf :Class,
        :Extendable_Class .

:Abstract_Method a owl:Class ;
    rdfs:subClassOf :Method_\(Member_Function\),
        :Overridable_Method .

:Accessor_Method a owl:Class ;
    rdfs:subClassOf :Instance_Method .

:Class_Method a owl:Class ;
    rdfs:subClassOf :Concrete_Method,
        :Non-Overridable_Method .

:Class_Variable a owl:Class ;
    rdfs:subClassOf :Attribute_\(Member_Variable\) .

:Concrete_Class a owl:Class ;
    rdfs:subClassOf :Class .

:Constructor_Method a owl:Class ;
    rdfs:subClassOf :Instance_Method,
        :Non-Overridable_Method .

:Destructor_Method a owl:Class ;
    rdfs:subClassOf :Instance_Method .

:Generic_Class a owl:Class ;
    rdfs:subClassOf :Class .

:Generic_Method a owl:Class ;
    rdfs:subClassOf :Method_\(Member_Function\) .

:Instance_Variable a owl:Class ;
    rdfs:subClassOf :Attribute_\(Member_Variable\) .

:Local_Variable a owl:Class ;
    rdfs:subClassOf :Method_Variable .

:Nested_Class a owl:Class ;
    rdfs:subClassOf :Class .

:Non-Extendable_Class a owl:Class ;
    rdfs:subClassOf :Class .

:Parameter_Variabe a owl:Class ;
    rdfs:subClassOf :Method_Variable .

:Primitive_Type a owl:Class ;
    rdfs:subClassOf :Type .

:Root_Class a owl:Class ;
    rdfs:subClassOf :Extendable_Class .

:Subclass a owl:Class ;
    rdfs:subClassOf :Class .

:Superclass a owl:Class ;
    rdfs:subClassOf :Extendable_Class .

:Overridable_Method a owl:Class ;
    rdfs:subClassOf :Method_\(Member_Function\) .

:Attribute_\(Member_Variable\) a owl:Class ;
    rdfs:subClassOf :Member,
        :Variable .

:Concrete_Method a owl:Class ;
    rdfs:subClassOf :Method_\(Member_Function\) .

:Member a owl:Class .

:Method_Variable a owl:Class ;
    rdfs:subClassOf :Variable .

:Non-Overridable_Method a owl:Class ;
    rdfs:subClassOf :Method_\(Member_Function\) .

:Type a owl:Class .

:Variable a owl:Class ;
    rdfs:subClassOf :Named_Element .

:Extendable_Class a owl:Class ;
    rdfs:subClassOf :Class .

:Instance_Method a owl:Class ;
    rdfs:subClassOf :Concrete_Method .

:Named_Element a owl:Class .

:Method_\(Member_Function\) a owl:Class ;
    rdfs:subClassOf :Member,
        :Named_Element .

:Class a owl:Class ;
    rdfs:subClassOf :Named_Element,
        :Type .

