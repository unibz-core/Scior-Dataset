@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Artefact_design a owl:Class ;
    rdfs:subClassOf :Plan_Description .

:Design_object a owl:Class ;
    rdfs:subClassOf :Object_Description .

:Object_Description a owl:Class ;
    rdfs:subClassOf :Normative_Description .

:Plan_Description a owl:Class ;
    rdfs:subClassOf :Normative_Description .

:Normative_Description a owl:Class .

