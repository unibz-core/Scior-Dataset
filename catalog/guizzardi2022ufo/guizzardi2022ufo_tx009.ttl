@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Husband a owl:Class ;
    rdfs:subClassOf :Man,
        :Spouse .

:Jogger a owl:Class ;
    rdfs:subClassOf :Person .

:Student a owl:Class ;
    rdfs:subClassOf :Person .

:Teacher a owl:Class ;
    rdfs:subClassOf :Person .

:Walker a owl:Class ;
    rdfs:subClassOf :Person .

:Wife a owl:Class ;
    rdfs:subClassOf :Spouse,
        :Woman .

:Man a owl:Class ;
    rdfs:subClassOf :Person .

:Woman a owl:Class ;
    rdfs:subClassOf :Person .

:Spouse a owl:Class ;
    rdfs:subClassOf :Person .

:Person a owl:Class .

