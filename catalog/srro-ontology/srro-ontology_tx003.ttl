@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Postcondition> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Condition> .

<http://taxonomy.model/Precondition> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Condition> .

<http://taxonomy.model/Condition> a owl:Class .

