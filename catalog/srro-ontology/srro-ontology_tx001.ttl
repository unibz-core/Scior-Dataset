@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:BusinessRule a owl:Class ;
    rdfs:subClassOf :Requirement .

:Class a owl:Class ;
    rdfs:subClassOf :EntityType .

:ClassTestCase a owl:Class ;
    rdfs:subClassOf :TestCase .

:FunctionalRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:Module a owl:Class ;
    rdfs:subClassOf :Artifact .

:NonFunctionalRequirement a owl:Class ;
    rdfs:subClassOf :Requirement .

:RequirementTestCase a owl:Class ;
    rdfs:subClassOf :TestCase .

:UseCase a owl:Class ;
    rdfs:subClassOf :Artifact .

:UseTestCase a owl:Class ;
    rdfs:subClassOf :TestCase .

:EntityType a owl:Class ;
    rdfs:subClassOf :Artifact .

:Requirement a owl:Class ;
    rdfs:subClassOf :Artifact .

:TestCase a owl:Class ;
    rdfs:subClassOf :Artifact .

:Artifact a owl:Class .

