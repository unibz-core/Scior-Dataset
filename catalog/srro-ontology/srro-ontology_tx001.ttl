@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/BusinessRule> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/Class> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/EntityType> .

<http://taxonomy.model/ClassTestCase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/TestCase> .

<http://taxonomy.model/FunctionalRequirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/Module> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/NonFunctionalRequirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Requirement> .

<http://taxonomy.model/RequirementTestCase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/TestCase> .

<http://taxonomy.model/UseCase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/UseTestCase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/TestCase> .

<http://taxonomy.model/EntityType> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/Requirement> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/TestCase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/Artifact> a owl:Class .

