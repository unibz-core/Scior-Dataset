@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ConditionChange> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ConditionAction> .

<http://taxonomy.model/ConditionCreation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ConditionAction> .

<http://taxonomy.model/ConditionTermination> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ConditionAction> .

<http://taxonomy.model/ConditionAction> a owl:Class .

