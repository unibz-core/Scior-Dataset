@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/AtomicAgentAction> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/AgentAction> .

<http://taxonomy.model/Interaction> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ComplexAgentAction> .

<http://taxonomy.model/ObjectAgentInteraction> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ComplexAgentAction> .

<http://taxonomy.model/AgentAction> a owl:Class .

<http://taxonomy.model/ComplexAgentAction> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/AgentAction> .

