@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ResourceChange> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResourceParticipation> .

<http://taxonomy.model/ResourceCreation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResourceParticipation> .

<http://taxonomy.model/ResourceTermination> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResourceParticipation> .

<http://taxonomy.model/ResourceUsage> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResourceParticipation> .

<http://taxonomy.model/ResourceParticipation> a owl:Class .

