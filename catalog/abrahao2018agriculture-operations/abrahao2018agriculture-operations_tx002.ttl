@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Payment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ValueActivity> .

<http://taxonomy.model/Production> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ValueActivity> .

<http://taxonomy.model/Purchase> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ValueActivity> .

<http://taxonomy.model/Receipt> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ValueActivity> .

<http://taxonomy.model/Sale> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ValueActivity> .

<http://taxonomy.model/ValueActivity> a owl:Class .

