@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ExceptionEvent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/TriggerEvent> .

<http://taxonomy.model/PlannedOrdinaryEvent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/OrdinaryEvent> .

<http://taxonomy.model/UnplannedOrdinaryEvent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/OrdinaryEvent> .

<http://taxonomy.model/OrdinaryEvent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/TriggerEvent> .

<http://taxonomy.model/TriggerEvent> a owl:Class .

