@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Bank a owl:Class ;
    rdfs:subClassOf :Organization .

:Client_Organization a owl:Class ;
    rdfs:subClassOf :Client,
        :Organization .

:Client_Person a owl:Class ;
    rdfs:subClassOf :Client,
        :Person .

:Organization_Member a owl:Class ;
    rdfs:subClassOf :Organization .

:Person a owl:Class .

:Client a owl:Class .

:Organization a owl:Class .

