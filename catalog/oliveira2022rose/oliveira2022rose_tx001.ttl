@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Attacker a owl:Class ;
    rdfs:subClassOf :Agent,
        :Threat_Object .

:Risk_Assessor a owl:Class ;
    rdfs:subClassOf :Agent .

:Risk_Subject a owl:Class ;
    rdfs:subClassOf :Agent .

:Value_Subject a owl:Class ;
    rdfs:subClassOf :Agent .

:Threat_Object a owl:Class .

:Agent a owl:Class .

