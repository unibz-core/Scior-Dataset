@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Graduate_Student> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Student> .

<http://taxonomy.model/Intern> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative> .

<http://taxonomy.model/Professor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Employee> .

<http://taxonomy.model/Regular> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Administrative> .

<http://taxonomy.model/Undergraduate_Student> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Student> .

<http://taxonomy.model/Administrative> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Employee> .

<http://taxonomy.model/Employee> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Person> a owl:Class .

<http://taxonomy.model/Student> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

