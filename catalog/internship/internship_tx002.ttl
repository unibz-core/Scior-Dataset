@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:College_Student a owl:Class ;
    rdfs:subClassOf :Student .

:Educational_Institution a owl:Class ;
    rdfs:subClassOf :Employer .

:Educational_Supervisor a owl:Class ;
    rdfs:subClassOf :Employee .

:High_School_Student a owl:Class ;
    rdfs:subClassOf :Student .

:Insurance_Carrier a owl:Class ;
    rdfs:subClassOf :Juridical_Person .

:Intern a owl:Class ;
    rdfs:subClassOf :Student .

:Internship_Provider_Organization a owl:Class ;
    rdfs:subClassOf :Employer,
        :Organization_Insured .

:Post_Graduate_Student a owl:Class ;
    rdfs:subClassOf :Student .

:Supervisor a owl:Class ;
    rdfs:subClassOf :Employee .

:Organization_Insured a owl:Class .

:Employee a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Employer a owl:Class ;
    rdfs:subClassOf :Juridical_Person .

:Juridical_Person a owl:Class ;
    rdfs:subClassOf :Legal_Person .

:Legal_Person a owl:Class .

:Natural_Person a owl:Class ;
    rdfs:subClassOf :Legal_Person .

:Student a owl:Class ;
    rdfs:subClassOf :Natural_Person .

