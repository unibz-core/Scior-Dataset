@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Adaptation_Sink a owl:Class ;
    rdfs:subClassOf :Adaptation_Function .

:Adaptation_Source a owl:Class ;
    rdfs:subClassOf :Adaptation_Function .

:Flow_Termination_Sink a owl:Class ;
    rdfs:subClassOf :Flow_Termination_Function .

:Flow_Termination_Source a owl:Class ;
    rdfs:subClassOf :Flow_Termination_Function .

:Adaptation_Function a owl:Class ;
    rdfs:subClassOf :Transport_Processing_Function .

:Flow_Termination_Function a owl:Class ;
    rdfs:subClassOf :Transport_Processing_Function .

:Transport_Processing_Function a owl:Class .

