@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Adaptation_Sink> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Adaptation_Function> .

<http://taxonomy.model/Adaptation_Source> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Adaptation_Function> .

<http://taxonomy.model/Flow_Termination_Sink> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Flow_Termination_Function> .

<http://taxonomy.model/Flow_Termination_Source> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Flow_Termination_Function> .

<http://taxonomy.model/Adaptation_Function> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transport_Processing_Function> .

<http://taxonomy.model/Flow_Termination_Function> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Transport_Processing_Function> .

<http://taxonomy.model/Transport_Processing_Function> a owl:Class .

