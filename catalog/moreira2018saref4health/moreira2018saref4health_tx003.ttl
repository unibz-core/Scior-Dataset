@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ECGDevice> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Device> .

<http://taxonomy.model/ECGLeadBipolarLimb> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ECGLead> .

<http://taxonomy.model/ECGLeadUnipolar> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ECGLead> .

<http://taxonomy.model/Sensor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Device> .

<http://taxonomy.model/Device> a owl:Class .

<http://taxonomy.model/ECGLead> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Sensor> .

