@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:CategorematicConcept a owl:Class ;
    rdfs:subClassOf :Concept .

:CategorematicTerm a owl:Class ;
    rdfs:subClassOf :MixedTerm,
        :Spoken-WrittenTerm .

:ComplexConcept a owl:Class ;
    rdfs:subClassOf :Concept .

:MentalSuppositum a owl:Class ;
    rdfs:subClassOf :Concept .

:SemanticallyComplexExpression a owl:Class ;
    rdfs:subClassOf :Expression .

:SemanticallyComplexSpoken-WrittenTerm a owl:Class ;
    rdfs:subClassOf :Spoken-WrittenTerm .

:SemanticallySimple_Expression a owl:Class ;
    rdfs:subClassOf :Expression .

:Spoken-WrittenSuppositum a owl:Class ;
    rdfs:subClassOf :Spoken-WrittenTerm .

:Suppositum a owl:Class ;
    rdfs:subClassOf :Term .

:SyncategorematicConcept a owl:Class ;
    rdfs:subClassOf :Concept .

:SyncategorematicTerm a owl:Class ;
    rdfs:subClassOf :MixedTerm,
        :Spoken-WrittenTerm .

:Expression a owl:Class ;
    rdfs:subClassOf :Spoken-WrittenTerm .

:MixedTerm a owl:Class .

:Term a owl:Class .

:Concept a owl:Class ;
    rdfs:subClassOf :Term .

:Spoken-WrittenTerm a owl:Class ;
    rdfs:subClassOf :Term .

