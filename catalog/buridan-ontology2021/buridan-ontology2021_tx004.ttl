@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ImproperSupposition a owl:Class ;
    rdfs:subClassOf :SuppositionInConventionalLanguage .

:MaterialSupposition a owl:Class ;
    rdfs:subClassOf :ProperSuppositon,
        :Supposition .

:MaterialSuppositionForSyncatogramaticTerm a owl:Class ;
    rdfs:subClassOf :Supposition .

:MentalPersonalSupposition a owl:Class ;
    rdfs:subClassOf :Supposition .

:PersonalSupposition a owl:Class ;
    rdfs:subClassOf :ProperSuppositon,
        :Supposition .

:SimpleSupposition a owl:Class ;
    rdfs:subClassOf :ProperSuppositon .

:SuppositionInConventionalLanguage a owl:Class ;
    rdfs:subClassOf :Supposition .

:ProperSuppositon a owl:Class ;
    rdfs:subClassOf :SuppositionInConventionalLanguage .

:Supposition a owl:Class .

