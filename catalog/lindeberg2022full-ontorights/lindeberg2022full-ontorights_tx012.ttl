    rdfs:subClassOf <http://taxonomy.model/UFO-B:_Counter-factual_situation> .

<http://taxonomy.model/Counter-factual_Consequence_For_Agent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Consequence_For_Agent> .

<http://taxonomy.model/UFO-B:_Counter-factual_situation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-B:_Situation> .

<http://taxonomy.model/Consequence_For_Agent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO-B:_Situation> .

<http://taxonomy.model/UFO-B:_Situation> a owl:Class .

