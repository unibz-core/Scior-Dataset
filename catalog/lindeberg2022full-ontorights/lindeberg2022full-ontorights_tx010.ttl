@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Complete_Ownership> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ownership> .

<http://taxonomy.model/Goods_Flow> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Supply_Flow> .

<http://taxonomy.model/Information_Flow> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Network_Connection> .

<http://taxonomy.model/Money_Flow> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Network_Connection> .

<http://taxonomy.model/Neutral> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Has_Influence_On> .

<http://taxonomy.model/Oppose> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Has_Influence_On> .

<http://taxonomy.model/Partial_Ownership> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ownership> .

<http://taxonomy.model/Service_Flow> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Supply_Flow> .

<http://taxonomy.model/Support> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Has_Influence_On> .

<http://taxonomy.model/Ownership> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Network_Connection> .

<http://taxonomy.model/Supply_Flow> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Network_Connection> .

<http://taxonomy.model/Has_Influence_On> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Network_Connection> .

<http://taxonomy.model/Network_Connection> a owl:Class .

