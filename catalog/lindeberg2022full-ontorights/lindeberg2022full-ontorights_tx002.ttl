@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Alleged_Human_Rights_Violation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Rights_Violation> .

<http://taxonomy.model/Benign_Human_Rights_Problem> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Rights_Problem> .

<http://taxonomy.model/Confirmed_Human_Rights_Violation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Rights_Violation> .

<http://taxonomy.model/Possible_Human_Rights_Violation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Rights_Violation> .

<http://taxonomy.model/Human_Rights_Problem> a owl:Class .

<http://taxonomy.model/Human_Rights_Violation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Human_Rights_Problem> .

