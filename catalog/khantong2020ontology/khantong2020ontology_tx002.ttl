@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:FloodDefenseManager a owl:Class ;
    rdfs:subClassOf :FloodDefenseOrganization .

:FloodDefenseMeasuresImplementer a owl:Class ;
    rdfs:subClassOf :FloodDefenseOrganization .

:FloodEvacuationImplementer a owl:Class ;
    rdfs:subClassOf :EvacuationOrganization .

:FloodEvacuationManager a owl:Class ;
    rdfs:subClassOf :EvacuationOrganization .

:FloodForecaster a owl:Class ;
    rdfs:subClassOf :FloodManagementOrganization .

:HighWaterController a owl:Class ;
    rdfs:subClassOf :FloodManagementOrganization .

:HighWaterProtectionImplementer a owl:Class ;
    rdfs:subClassOf :FloodManagementOrganization .

:HighWaterProtectionMeasureEvaluator a owl:Class ;
    rdfs:subClassOf :FloodManagementOrganization .

:WeatherForecaster a owl:Class ;
    rdfs:subClassOf :WeatherOrganization .

:WeatherOrganization a owl:Class ;
    rdfs:subClassOf :Organization .

:EvacuationOrganization a owl:Class ;
    rdfs:subClassOf :Organization .

:FloodDefenseOrganization a owl:Class ;
    rdfs:subClassOf :Organization .

:FloodManagementOrganization a owl:Class ;
    rdfs:subClassOf :Organization .

:Organization a owl:Class .

