@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Ethical_Normative_Description> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ethical_Object>,
        <http://taxonomy.model/Normative_Description> .

<http://taxonomy.model/Principle> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ethical_Norm> .

<http://taxonomy.model/Rule> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ethical_Norm> .

<http://taxonomy.model/Teaching> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ethical_Norm> .

<http://taxonomy.model/Normative_Description> a owl:Class .

<http://taxonomy.model/Social_Object> a owl:Class .

<http://taxonomy.model/Ethical_Object> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Social_Object> .

<http://taxonomy.model/Ethical_Norm> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Ethical_Object> .

