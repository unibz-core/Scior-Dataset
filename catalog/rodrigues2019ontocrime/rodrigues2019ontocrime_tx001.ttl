@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Crime_Author a owl:Class ;
    rdfs:subClassOf :Active_Agent,
        :Criminally_Liable .

:Criminally_Incapable a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Guaranteed a owl:Class ;
    rdfs:subClassOf :Physical_Agent .

:Guarantor_Able_Person a owl:Class ;
    rdfs:subClassOf :Able_Person,
        :Guarantor .

:Jurisdictional_State a owl:Class ;
    rdfs:subClassOf :Jurisdiction_Agent,
        :State .

:Mentally_Sick a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Passive_Juridical_Person a owl:Class ;
    rdfs:subClassOf :Juridical_Person,
        :Passive_Agent .

:Passive_Person a owl:Class ;
    rdfs:subClassOf :Natural_Person,
        :Passive_Agent .

:Passive_Society a owl:Class ;
    rdfs:subClassOf :Passive_Agent,
        :Society .

:Underage a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Able_Person a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Active_Agent a owl:Class ;
    rdfs:subClassOf :Crime_Agent,
        :Legal_Agent,
        :Natural_Person .

:Adult a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Collective_Social_Agent a owl:Class ;
    rdfs:subClassOf :Social_Agent .

:Criminally_Liable a owl:Class ;
    rdfs:subClassOf :Adult,
        :Mentally_Healthy .

:Guarantor a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Juridical_Person a owl:Class ;
    rdfs:subClassOf :Institutional_Agent .

:Jurisdiction_Agent a owl:Class ;
    rdfs:subClassOf :Legal_Agent .

:Mentally_Healthy a owl:Class ;
    rdfs:subClassOf :Natural_Person .

:Society a owl:Class ;
    rdfs:subClassOf :Collective_Social_Agent .

:State a owl:Class ;
    rdfs:subClassOf :Institutional_Agent .

:Crime_Agent a owl:Class ;
    rdfs:subClassOf :Legal_Agent .

:Institutional_Agent a owl:Class ;
    rdfs:subClassOf :Social_Agent .

:Physical_Agent a owl:Class ;
    rdfs:subClassOf :Agent .

:Social_Agent a owl:Class ;
    rdfs:subClassOf :Agent .

:Passive_Agent a owl:Class ;
    rdfs:subClassOf :Crime_Agent,
        :Legal_Agent .

:Agent a owl:Class .

:Legal_Agent a owl:Class ;
    rdfs:subClassOf :Agent .

:Natural_Person a owl:Class ;
    rdfs:subClassOf :Agent,
        :Physical_Agent .

