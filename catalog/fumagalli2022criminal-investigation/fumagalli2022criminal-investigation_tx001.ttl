@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Junior_Detective a owl:Class ;
    rdfs:subClassOf :Detective .

:Superintendent a owl:Class ;
    rdfs:subClassOf :Lead_Detective,
        :Senior_Detective .

:Suspect a owl:Class ;
    rdfs:subClassOf :Person .

:Witness a owl:Class ;
    rdfs:subClassOf :Person .

:Lead_Detective a owl:Class ;
    rdfs:subClassOf :Detective .

:Senior_Detective a owl:Class ;
    rdfs:subClassOf :Detective .

:Detective a owl:Class ;
    rdfs:subClassOf :Person .

:Person a owl:Class .

