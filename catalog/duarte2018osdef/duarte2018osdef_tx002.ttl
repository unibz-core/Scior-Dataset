@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/User_False_Belief> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Belief> .

<http://taxonomy.model/User_Malicious_Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Belief> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Moment> .

<http://taxonomy.model/Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Mental_Moment> .

<http://taxonomy.model/Mental_Moment> a owl:Class .

