@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Fault_(Runtime_Defect)> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Defect> .

<http://taxonomy.model/Hardware_Vulnerability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Vulnerability> .

<http://taxonomy.model/Loaded_Program_Vulnerability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Vulnerability> .

<http://taxonomy.model/Usage_Limit_Vulnerability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Vulnerability> .

<http://taxonomy.model/Defect> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Vulnerability> .

<http://taxonomy.model/Disposition> a owl:Class .

<http://taxonomy.model/Vulnerability> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Disposition> .

