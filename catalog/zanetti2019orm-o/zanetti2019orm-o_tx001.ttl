@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Entity_Class> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Class> .

<http://taxonomy.model/Entity_Subclass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Subclass> .

<http://taxonomy.model/Entity_Superclass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Spuerclass> .

<http://taxonomy.model/Extendable_Class> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Class> .

<http://taxonomy.model/Spuerclass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Extendable_Class> .

<http://taxonomy.model/Subclass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Class> .

<http://taxonomy.model/Class> a owl:Class .

