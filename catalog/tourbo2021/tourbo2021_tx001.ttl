@prefix : <http://taxonomy.model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Climbing a owl:Class ;
    rdfs:subClassOf :sport_activity .

:Cycling a owl:Class ;
    rdfs:subClassOf :sport_activity .

:Hiking a owl:Class ;
    rdfs:subClassOf :sport_activity .

:Skiing a owl:Class ;
    rdfs:subClassOf :sport_activity .

:Winery_visit a owl:Class ;
    rdfs:subClassOf :food_and_wine_tour .

:castel_visit a owl:Class ;
    rdfs:subClassOf :cultural_visit .

:church_visit a owl:Class ;
    rdfs:subClassOf :cultural_visit .

:city_center_visit a owl:Class ;
    rdfs:subClassOf :cultural_visit .

:museum_visit a owl:Class ;
    rdfs:subClassOf :cultural_visit .

:resturant_visit a owl:Class ;
    rdfs:subClassOf :food_and_wine_tour .

:food_and_wine_tour a owl:Class ;
    rdfs:subClassOf :activity .

:activity a owl:Class .

:cultural_visit a owl:Class ;
    rdfs:subClassOf :activity .

:sport_activity a owl:Class ;
    rdfs:subClassOf :activity .

