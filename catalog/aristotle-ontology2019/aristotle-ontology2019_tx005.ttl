@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Future> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Time> .

<http://taxonomy.model/Past> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Time> .

<http://taxonomy.model/Present> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Time> .

<http://taxonomy.model/Number> a owl:Class .

<http://taxonomy.model/Time> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Number> .

