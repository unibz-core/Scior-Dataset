@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Aristocracy> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CorrectForm> .

<http://taxonomy.model/Democraty> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/DeviantForm> .

<http://taxonomy.model/Kingship> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CorrectForm> .

<http://taxonomy.model/Oligarchy> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/DeviantForm> .

<http://taxonomy.model/Polity> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/CorrectForm> .

<http://taxonomy.model/Tiranny> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/DeviantForm> .

<http://taxonomy.model/ConstitutionalForm> a owl:Class .

<http://taxonomy.model/CorrectForm> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ConstitutionalForm> .

<http://taxonomy.model/DeviantForm> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ConstitutionalForm> .

