@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Lawgiver> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Politician> .

<http://taxonomy.model/Non-Citizen> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResidentPerson> .

<http://taxonomy.model/Ruler> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Politician> .

<http://taxonomy.model/Citizen> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/ResidentPerson> .

<http://taxonomy.model/Politician> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Citizen> .

<http://taxonomy.model/ResidentPerson> a owl:Class .

