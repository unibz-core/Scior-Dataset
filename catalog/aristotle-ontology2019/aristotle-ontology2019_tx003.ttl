@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ActivePotentiality> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Potentiality> .

<http://taxonomy.model/PassivePotentiality> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Potentiality> .

<http://taxonomy.model/Potentiality> a owl:Class .

