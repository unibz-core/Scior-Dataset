@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Affirmation> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assertion> .

<http://taxonomy.model/Conclusion> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assertion> .

<http://taxonomy.model/Denial> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assertion> .

<http://taxonomy.model/Premise> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Assertion> .

<http://taxonomy.model/Assertion> a owl:Class .

