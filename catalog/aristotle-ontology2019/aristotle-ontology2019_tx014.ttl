@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Particular> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Species> .

<http://taxonomy.model/Genus> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/SupremeGenus-HighestKind> .

<http://taxonomy.model/Species> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Genus> .

<http://taxonomy.model/SupremeGenus-HighestKind> a owl:Class .

