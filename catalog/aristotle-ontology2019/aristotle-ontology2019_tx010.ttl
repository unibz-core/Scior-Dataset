@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Extreme> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Term> .

<http://taxonomy.model/MiddleTerm> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Term> .

<http://taxonomy.model/Predicate> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Term> .

<http://taxonomy.model/Subject> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Term> .

<http://taxonomy.model/Term> a owl:Class .

